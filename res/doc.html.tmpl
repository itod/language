{% for func in funcs %}
<div>
    <p>
        <i>{{func.returnType}}</i> <b>{{func.name}}</b>
        ({% for param in func.params %}{{param.name}}{% if not currentLoop.isLast %}, {% /if %}{% /for %})
    </p>
    <p>
        <blockquote>
            <p></p>
        </blockquote>
    </p>
</div>
{% /for %}